    "picocolors": "^1.0.0",
    "rgb-hex": "^4.0.1",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config": "^0.3.15",
    "@pengzhanbo/prettier-config": "^0.3.15",
    "@types/debug": "^4.1.8",
    "@types/node": "^18.16.19",
    "@types/sharp": "^0.32.0",
    "bumpp": "^9.1.1",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^8.44.0",
    "prettier": "^2.8.8",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "peerDependencies": {
    "vite": ">=3.0.0"
  },
  "packageManager": "pnpm@8.6.5",
  "engines": {
    "node": "^14.18.0 || >=16"
  }
